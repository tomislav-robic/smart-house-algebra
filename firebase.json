{
  "functions": {
      "predeploy": [
          "npm --prefix \"%RESOURCE_DIR%\" run lint"
      ],
      "source": "functions"
  },
  "hosting": {
      "public": "dist/smart-house-algebra",
      "ignore": [
          "firebase.json",
          "**/.*",
          "**/node_modules/**"
      ],
      "rewrites": [{
              "source": "/public/**",
              "destination": "/public.html"
          },
          {
              "source": "**",
              "destination": "/index.html"
          }
      ],
      "storage": {
          "rules": "storage.rules"
      }
  },
  "budgets": [
    {
      "type": "initial",
      "maximumWarning": "4mb",
      "maximumError": "5mb"
    },
    {
      "type": "anyComponentStyle",
      "maximumWarning": "2kb",
      "maximumError": "4kb"
    }
  ]
  }